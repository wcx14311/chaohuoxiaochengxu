.user-photo{
  text-align: center;
  margin-bottom: 10px;
}
.user-photo image{
  width: 70px;
  height: 70px;
  border: 2px solid #fff;
  border-radius: 50%;
}
.u-toji{
  display: flex;
}
.u-double{
  padding:15px 0;
  flex: 1;
  text-align: center;
  position: relative;
}
.u-double .c1{
  font-size: 20px;
  padding-bottom:5px;
  color:#f33333;
}
.u-double .c2{
  font-size:16px;
}
.u-border-r::after{
  width: 1px;
  height: 100%;
  position:absolute;
  background: #ededed;
  content: '';
  top:0;
  right: 0;
}
.button-section{
  padding:30rpx 30rpx 80rpx;
}
.button-section .btn{
  height: 40px;
  line-height: 40px;
  padding: 0;
  text-align: center;
}
.button-section .red-fill{
  background: #f43334;
  color:#fff;
}
.button-section .red-fill::after{
  border:none;
}
.button-section .red-fill.button-hover{
  background: #c20d0d;
}
