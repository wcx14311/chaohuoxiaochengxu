/* pages/order/orderDetail/index.wxss */
.top-msg{
  background:#f33333;
  color:#fff;
  padding:5px 20rpx;
  font-size:12px;
}
.sanfen-block{
  display: flex;
  border-bottom:10px solid #ededed;
}
.sanfen-block .csx{
  text-align: center;
  flex: 1;
  padding: 20px 0;
  position:relative;
}
.sanfen-block .csx .tx{
  color: #a0a0a0;
}
.sanfen-block .csx .vcs{
  height: 30px;
  margin-top: 5px;
  line-height:30px;
}
.sanfen-block .csx .vcs button{
  line-height:30px;
  height: 30px;
  display: inline-block;
  font-size: 12px;
  color:#f33333;
}
.honzi{
  color: #f33333;
}
.heizi{
  color: #000;
}
.sanfen-block .csx .sct{
  background: url("data:image/png;base64,") no-repeat;
  background-size:13px;
  padding-left:15px;
  background-position: left center;
}
.sanfen-block .csx .scb{
  background: url("data:image/png;base64,") no-repeat;
  background-position: left center;
  background-size:15px;
  padding-left:18px;
}
.sanfen-block .csx .scs{
  background: url("data:image/png;base64,") no-repeat;
  background-position: left center;
  background-size:15px;
  padding-left:18px;
}
.sanfen-block .csx.b::after{
  position:absolute;
  top:0;
  right: 0;
  width:1px;
  height:100%;
  content:'';
  background:#ededed;
}
.order-info{
  border-bottom:10px solid #ededed;
}
.order-info .order-user-info{
  margin:0 20rpx;
  padding:15px 0;
}
.order-info .order-user-info .scx{
  padding-left:25px;
  background: url("data:image/png;base64,") no-repeat;
  background-size:18px;
  background-position:0px 2px;
}
.order-info .order-user-info .scv{
  padding-left:25px;
}
.order-info .order-money{
  line-height:50px;
  text-align: center;
  border-top: 1px solid #ededed;
}
.order-info .order-money text{
  color:#a8a8a8;
}
.phone-list{
  border-top: 1px solid #ededed;
}
.phone-order-no{
  line-height:50px;
  padding: 0 20rpx;
  border-bottom: 1px solid #ededed;
  text-overflow:ellipsis;
  white-space:nowrap;
  overflow: hidden; 
}
.phone-order-time{
  line-height:50px;
  padding: 0 20rpx;
  text-overflow:ellipsis;
  white-space:nowrap;
  overflow: hidden;
}
.phone-item{
  position: relative;
  display: flex;
  display: -webkit-box;
  border-bottom: 1px solid #ededed;
}
.phone-item .item-image{
  padding: 10px 0;
  margin-left: 20rpx;
}
.phone-item .item-image image{
  display: block;
  width:70px;
  height:70px;
}
.phone-item .item-text{
  flex: 1;
  -webkit-box-flex: 1;
  margin-right: 20rpx;
  padding:10px 0 10px 10px;
}
.phone-item .num::before{
  font-size: 12px;
  content: "Ã—";
}
.phone-item .num{
  top:30px;
  right: 20rpx;
  position: absolute;
}
.phong-xihao{
  font-size: 14px;
  padding-top: 5px;
  color:#a8a8a8;
}
.phone-orader-btn{
  padding:10px 0 40px;
  display: flex;
}
.phone-orader-btn .btnf{
  flex: 1;
  padding: 0 20rpx;
}
.fill-btn{
  background: #f33333;
  color: #fff;
}
.border-btn{
  background: #fff;
  color:#f33333;
}
.border-btn::after{
  border:2px solid #f33333;
  width: 100%;
  height:100%;
  -webkit-transform:scale(1);
  transform:scale(1);
}
.phone-orader-btn .one{
  width: 100%;
  box-sizing: border-box;
  padding: 0 20rpx;
}