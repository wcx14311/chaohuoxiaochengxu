/**index.wxss**/
.cenmane{
  width:200rpx;
  height:200rpx;
  border: 2px solid #fff;
  border-radius: 50%;
  padding:10rpx;
}
.namumber{
  width:200rpx;
  height:200rpx;
  background: #fff;
  border-radius: 50%;
  color: #f43334;
  text-align: center;
}
.namumber text{
  display: block;
  text-align: center;
}
.big-text{
  padding-top:30rpx;
  font-size:100rpx;
  line-height:100rpx;
}
.text-label{
  font-size:30rpx;
}
.checkAgentButton{
  height:40px;
  line-height:35px;
  padding-bottom:0;
  background: transparent;
  border: 2px solid #fff;
  color: #fff;
  width: 100%;
  display: block;
  margin-top:30rpx;
}
.userinfo {
  display: flex;
  flex-direction: column;
  align-items: center;
}

.userinfo-avatar {
  width: 128rpx;
  height: 128rpx;
  margin: 20rpx;
  
}

.userinfo-nickname {
  color: #aaa;
}

.usermotto {
  margin-top: 200px;
}
.dialog-bzj.hide{
  display: none;
}
.dialog-bzj .bzj-info{
  position: absolute;
  left: 50%;
  top:50%;
  transform:translate(-50%,-50%);
  width:220px;
  background: #fff;
  border-radius: 10px;
  box-shadow: 0 0 10px rgba(0,0,0,0.5);
  padding:20px 20px 30px 20px;
  text-align: center;
}
.dialog-bzj .bzj-info .t1{
  font-size: 20px;
  margin-bottom: 10px;
}
.dialog-bzj .bzj-info .t2{
  font-size: 12px;
  margin-bottom:15px;
}
.dialog-bzj .bzj-info .t3{
  font-size: 25px;
}
.dialog-bzj .bzj-info .t4{
  font-size: 25px;
  color: #f43334;
}
.dialog-bzj .bzj-info .t5{
  margin-bottom: 10px;
  display: flex;
}
.dialog-bzj .bzj-info .t5 .c{
  flex: 1;
}
.line{
  border-top: 1px dashed #f43334;
  margin-left: -20px;
  margin-right: -13px;
  margin-top:12px;
}
.dialog-bzj .bzj-info .t5 .a .y{
  display: inline-block;
  width:20px;
  height: 20px;
  text-align: center;
  color: #f43334;
  border: 1px solid #f43334;
  line-height: 20px;
  border-radius: 50%;
}
.dialog-bzj .bzj-info .t5 .a .s{
  font-size: 12px;
  margin-top: 10px;
}
.dialog-bzj .bzj-info .tc{
  background: #f43334;
  color: #fff;
  line-height: 40px;
  height: 40px;
}
.dialog-bzj .bzj-info .button-hover{
  background: #b40b0b;
}